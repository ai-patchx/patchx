{
  "name": "patchx",
  "private": true,
  "version": "0.22.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "node dev-start.js",
    "dev:env": "TEST_USER_PASSWORD=test123 vite",
    "build": "tsc -b && vite build",
    "build:worker": "tsc -p tsconfig.worker.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b --noEmit",
    "db:reset": "bash scripts/reset-db.sh",
    "db:reset:confirm": "bash scripts/reset-db.sh --confirm",
    "sync:env": "node scripts/sync-env-to-wrangler.js",
    "test:config": "bash scripts/test-config-endpoint.sh",
    "check:env": "bash scripts/check-env-setup.sh",
    "deploy": "npm run build:worker && npx wrangler deploy --env production",
    "deploy:full": "npm run sync:env && npm run deploy"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.83.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "tailwind-merge": "^3.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251115.0",
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vercel/node": "^5.5.6",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "sucrase": "^3.35.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.50.0"
  }
